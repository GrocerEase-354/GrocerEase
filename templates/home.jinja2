{% extends "navbar.jinja2" %}
{% block title %}
Home
{% endblock title%}

{% block content %}
    <h1>Home: Select a button to check a specific category</h1>
    <form method="post" id="buttons">
    
    
        <button type="submit" name="submitbutton" class="inlineBlockButton" value="All" class="btn btn-warning">All</button>
        {% for i in retVal %}
            {% for j in i %}
            <button type="submit" name="submitbutton" class="inlineBlockButton" value="{{ j|string() }}" class="btn btn-warning">{{ j|string() }}</button>
            {% endfor %}
            
        {% endfor %}
    </form>
{% endblock content %}